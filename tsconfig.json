{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "rollup/parseAst": ["./src/types/rollup.d.ts"] // Add this line
    },
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node16", // Ensure this line is present
    "module": "Node16", // Ensure this line is present
    "target": "ESNext", // Ensure this line is present
    "lib": ["DOM", "DOM.Iterable", "ESNext"], // Ensure this line is present
    "strict": true, // Ensure this line is present
    "skipLibCheck": true, // Ensure this line is present
    "forceConsistentCasingInFileNames": true // Ensure this line is present
  }
}